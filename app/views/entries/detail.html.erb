<% provide(:title, "Detail") %>
<h1>"<%= @entry.category %>"的細節</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2 col-xs-10 col-xs-offset-1">
    <%= form_for(@entry) do |f| %>
      <%= render 'full_form_fields', f: f %>
      <%= f.submit "Modify This Record確認修改", class: "btn btn-primary" %>
      <%= link_to "Cancel", root_path %>
    <% end %>
  </div>
</div>